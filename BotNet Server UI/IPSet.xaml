<Window x:Class="BotNet_Server_UI.IPSet"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Окно ввода" Height="97" Width="375" WindowState="Normal" ResizeMode="CanMinimize">
    <StackPanel>
        <TextBlock Text="Введите все ID через пробел (all для всех, null - очистить)" />
        <TextBox x:Name="ResponseTextBox" KeyDown="Field_KeyDown" />
        <Button x:Name="OKButton" Content="OK" Click="OKButton_Click" />
    </StackPanel>
</Window>
